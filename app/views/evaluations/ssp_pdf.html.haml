/ Content Header (Page header)
/ Main content
%section.content.container-fluid
  .row.body
    .col-md-12
      .box.box-primary
        .box-header.with-border
          %h2.box-title SSP Ready Control Implementation Details and Status
          %h2.box-title= evaluation.profiles.map(&:name).join(', ')
          .box-body
            - counts, _, _ = evaluation.status_counts
            - total_count = counts[:failed] + counts[:passed] + counts[:not_reviewed] + counts[:profile_error]
            - compliance_level = ((counts[:passed]).to_f / total_count) * 100
            %h3
              Compliance Level: #{compliance_level.round(1)}%
            %table.findings{:width => "80%"}
              %tr
                %td
                  Passed: #{counts[:passed]}
                %td
                  Failed: #{counts[:failed]}
                %td
                  Not Reviewed: #{counts[:not_reviewed]}
              %tr
                %td
                  Profile Error: #{counts[:profile_error]}
                %td
                  Not Applicable: #{counts[:not_applicable]}
      .box-header.with-border
        .box-body
          - nist_hash["children"].each do |family|
            - if family["value"] > 0
              %h3{:id => "#{family['name']}"}
                = family["name"]
                \: #{family["desc"]}
              .container-fluid
                - family["children"].each do |cf|
                  - if cf["value"] > 0
                    - cf_name = cf['name']
                    %h4{:id => cf_name, :onclick => "togAll('#{cf_name}')"}= cf_name
                    .container-fluid.big-margin-bottom
                      - cf["controls"].each_with_index do |control, index|
                        %h4
                          %button{:class => "#{status_btn(symbols[control.control_id])}", :onclick => "tog('#{control.control_id}')"}= symbols[control.control_id].to_s.titleize
                          = index+1
                          \. #{control.title}
                        .container-fluid.big-margin-bottom{:id => "#{control.control_id}"}
                          = control.desc
                          %h5 Check Text
                          %pre= control.tag('check')
                          %h5 Fix Text
                          %pre= control.tag('fix')
